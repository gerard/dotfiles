# vim: set filetype=sh :

JUMP_CONF="$HOME/.jumps"
_jump() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts=`awk '{ print $1 }' < $JUMP_CONF | tr '\n' ' '`

    [ ${prev} != "jump" ] && return

    COMPREPLY=( $(compgen -W "\${opts}" -- ${cur}) )
    return 0
}
complete -F _jump jump

